package com.webdevils {			public static class Embedded_Font {		public static function get_embedded_font( _font:String ):String {			var embedded_font_class:Class = getDefinitionByName( _font ) as Class;			Font.registerFont( embedded_font_class );			var embedded_font_array:Array = Font.enumerateFonts( false );			var embedded_font = embedded_font_array[ 0 ];			return embedded_font;		}	}}