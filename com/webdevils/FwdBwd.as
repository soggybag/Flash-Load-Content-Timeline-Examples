package com.webdevils {	import flash.display.*;	import flash.events.*;	public class FwdBwd extends MovieClip {		private var is_over:Boolean;		public function FwdBwd() {			is_over = false;			addEventListener( MouseEvent.ROLL_OVER, on_rollover );			addEventListener( MouseEvent.ROLL_OUT, on_rollout );			addEventListener( Event.ENTER_FRAME, on_enterframe );		}		private function on_rollover( evt:Event ):void {			is_over = true;		}		private function on_rollout( evt:Event ):void {						if ( evt.currentTarget.mouseEnabled == true ) {				is_over = false;			}			// trace( this, "out" );		}		private function on_enterframe( evt:Event ):void {			if ( is_over ) {				nextFrame();			} else {				prevFrame();			}		}	}}