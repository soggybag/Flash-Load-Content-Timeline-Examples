package com.webdevils {	import flash.text.*;	import flash.utils.*;		public class Font_Util {		public static function get_embedded_font( _font:String ):* {			var embedded_font_class:Class = getDefinitionByName( _font ) as Class;			Font.registerFont( embedded_font_class );			var embedded_font_array:Array = Font.enumerateFonts( false );			return embedded_font_array[ 0 ].fontName;		}	}}